#Options
GenerateScanner = true
GenerateParser = true
Language = Java
ScannerName = Lexico
ParserName = Sintatico
SemanticName = Semantico
Package = br.furb.jsondb.parser
ScannerCaseSensitive = false
ScannerTable = Compact
Input = String
Parser = LL
#RegularDefinitions
min: [a-z]
mai: [A-Z]
letra: {mai} | {min}
digito: [0-9]
id: _? {letra} (_? | {letra} | {digito})*
#Tokens
//identificadores
	id: {id}
	id_ref: \" {id} \"
	numero: (0 | [1-9] {digito}*) (".")? (0 | [1-9] {digito}*)?
	literal: \" ([^\\\n\"] | \\\" | \\\\)* \"
	data: {digito}{digito} / {digito}{digito} / {digito}{digito}{digito}{digito}

//simbolos especiais
	";"
	","
	"."
	"*"
	"("
	")"
	"="
	"<"
	"<="
	">"
	">="
	"<>"
	"null"

//palavras reservadas
	SET = id: "SET"
	DATABASE = id: "DATABASE"
	CREATE = id: "CREATE"
	DROP = id: "DROP"
	DESCRIBE = id: "DESCRIBE"
	TABLE  = id: "TABLE"
	CONSTRAINT = id: "CONSTRAINT"
	PRIMARY = id: "PRIMARY"
	FOREIGN = id: "FOREIGN"
	KEY = id: "KEY"
	REFERENCES = id: "REFERENCES"
	INSERT = id: "INSERT"
	INTO = id: "INTO"
	VALUES = id: "VALUES"
	SELECT = id: "SELECT"
	FROM = id: "FROM"
	WHERE = id: "WHERE"
	AND = id: "AND"
	OR = id: "OR"
	NUMBER = id: "NUMBER"
	VARCHAR = id: "VARCHAR"
	DATE = id: "DATE"
	CHAR = id: "CHAR"
	NULL = id: "NULL"
	NOT = id: "NOT"
	INDEX = id: "INDEX"

//ignorados
	:[\r\s\n\t]
#NonTerminals
<ACAO>
<CRIAR>
<CRIAR2>
<ATRIBUTOS>
<ATRIBUTO>
<RATRIBUTOS>
<RATRIBUTOS2>
<TIPO>
<RESTRICOES>
<RESTRICAO1>
<TIPO_RESTRICAO1>
<RESTRICAO2>
<TIPO_RESTRICAO2>
<RESTRICAO_FINAL>
<RRESTRICAO_FINAL>
<RESTRICAO3>
<TIPO_RESTRICAO3>
<DECIMAL>
<LISTAIDS>
<RLISTAIDS>
<INCLUIR>
<COLUNAS>
<LISTAVALORES>
<VALOR>
<RLISTAVALORES>
<SELECIONAR>
<CAMPOSNOVO>
<CAMPOS>
<RCAMPOS>
<CAMPO>
<ID>
<QUALIFICADOR>
<TABELAS>
<RTABELAS>
<TABELA>
<CLAUSULA_WHERE>
<CONDICOES>
<CONDICAO>
<RCONDICAO>
<OPERADOR_REL>
<OPERADOR_LOG>
<ELIMINAR>
<ELIMINAR2>
<DESCREVER>
<SETAR_BANCO>
#Grammar
<ACAO> ::= <CRIAR> | <INCLUIR> | <SELECIONAR> | <ELIMINAR> | <DESCREVER> | <SETAR_BANCO>;
<CRIAR> ::= CREATE <CRIAR2> ";";
<CRIAR2> ::= DATABASE <ID> #999 | TABLE <TABELA> "(" <ATRIBUTOS> ")" #58 | INDEX <TABELA> "(" <CAMPO> ")" #66 ;
<ATRIBUTOS> ::= <ATRIBUTO> <RATRIBUTOS>;
<ATRIBUTO> ::= <ID> <TIPO> #53 <RESTRICOES>;
<RATRIBUTOS> ::= "," <RATRIBUTOS2> | î;
<RATRIBUTOS2> ::= <ATRIBUTOS> | <RESTRICAO_FINAL> ;
<TIPO> ::= NUMBER #999 "(" numero <DECIMAL> #999 ")" | VARCHAR #999 "(" numero #999 ")" | DATE #999 | CHAR #999 "(" numero #999 ")";
<RESTRICOES> ::= CONSTRAINT <RESTRICAO1> | <RESTRICAO2> | î;
<RESTRICAO1> ::= id #15 <TIPO_RESTRICAO1> #55;
<TIPO_RESTRICAO1> ::= NULL | NOT NULL | PRIMARY KEY | REFERENCES <TABELA> "(" <LISTAIDS> ")";
<RESTRICAO2> ::= <TIPO_RESTRICAO2> #55;
<TIPO_RESTRICAO2> ::= NULL | NOT NULL | PRIMARY KEY | REFERENCES <TABELA> "(" <ID> ")";
<RESTRICAO_FINAL> ::= CONSTRAINT <RESTRICAO3> <RRESTRICAO_FINAL>;
<RRESTRICAO_FINAL> ::= "," <RESTRICAO_FINAL> | î;
<RESTRICAO3> ::= id #15 <TIPO_RESTRICAO3> #55;
<TIPO_RESTRICAO3> ::= PRIMARY KEY "(" <LISTAIDS> ")" | FOREIGN KEY "(" <LISTAIDS> ")" REFERENCES <TABELA> "(" <LISTAIDS> ")";
<DECIMAL> ::= "," numero | î;
<LISTAIDS> ::= <ID> <RLISTAIDS>;
<RLISTAIDS> ::= "," <LISTAIDS> | î;
<INCLUIR> ::= INSERT INTO <TABELA> <COLUNAS> VALUES "(" <LISTAVALORES> ")" ";" #98;
<COLUNAS> ::= "(" <LISTAIDS> ")" | î;
<LISTAVALORES> ::= <VALOR> #43 <RLISTAVALORES>;
<VALOR> ::= numero | literal | data | "null";
<RLISTAVALORES> ::= "," <LISTAVALORES> | î;
<SELECIONAR> ::= SELECT <CAMPOSNOVO> FROM <TABELAS> <CLAUSULA_WHERE> #99;
<CAMPOSNOVO> ::= <CAMPOS> | "*";
<CAMPOS> ::= <CAMPO> <RCAMPOS>;
<RCAMPOS> ::= "," <CAMPOS> | î;
<TABELAS> ::= <TABELA> <RTABELAS>;
<RTABELAS> ::= "," <TABELAS> | î;
<ID> ::= id #13;
<CAMPO> ::= <QUALIFICADOR> <ID>;
<QUALIFICADOR> ::= <TABELA> "." | î;
<TABELA> ::= id #18;
<CLAUSULA_WHERE> ::= WHERE <CONDICOES> | î;
<CONDICOES> ::= <CONDICAO> <RCONDICAO>;
<CONDICAO> ::= <CAMPO> <OPERADOR_REL> #97 <VALOR> #43;
<RCONDICAO> ::= <OPERADOR_LOG> <CONDICOES> | î;
<OPERADOR_REL> ::= "=" | ">" | "<" | ">=" | "<=" | "<>";
<OPERADOR_LOG> ::= AND | OR;
<ELIMINAR> ::= DROP <ELIMINAR2> ";";
<ELIMINAR2> ::= TABLE <TABELA> | INDEX <TABELA> "(" <CAMPO> ")" #67;
<DESCREVER> ::= DESCRIBE <TABELA> ";" #63;
<SETAR_BANCO> ::= SET DATABASE <ID> ";" #65;
